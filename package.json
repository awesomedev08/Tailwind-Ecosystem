{
    "name": "dd360-ds",
    "version": "1.0.31",
    "description": "DD360 Components is a tailwind based React component library",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "engines": {
        "node": ">=16",
        "npm": "7 || 8"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/dd3tech/dd360/dd3-components-ui"
    },
    "bugs": {
        "url": "https://gitlab.com/dd3tech/dd360/dd3-components-ui/-/issues"
    },
    "scripts": {
        "serve": "vite preview",
        "storybook": "start-storybook -p 6006",
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage",
        "prebuild-lib": "rimraf dist",
        "build-lib": "rollup -c",
        "build-storybook": "build-storybook",
        "format": "prettier --write ./src",
        "prepare": "husky install",
        "commit": "git cz",
        "add-co": "git add . && git cz",
        "pre-commit": "npm run format",
        "pre-push": "npm run test",
        "copy": "downlevel-dts dist dist/types",
        "clean": "rm -rf ./node_modules && rm -f package-lock.json"
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@heroicons/react": "^1.0.5",
        "dd360-utils": "^1.0.12",
        "react-circular-progressbar": "^2.1.0"
    },
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/plugin-external-helpers": "^7.18.6",
        "@babel/plugin-transform-react-jsx": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/plugin-transform-typescript": "^7.18.12",
        "@babel/preset-env": "^7.18.10",
        "@rollup/plugin-commonjs": "17.1.0",
        "@rollup/plugin-node-resolve": "11.2.0",
        "@rollup/plugin-babel": "^5.3.1",
        "@storybook/addon-actions": "6.5.10",
        "@storybook/addon-essentials": "6.5.10",
        "@storybook/addon-links": "6.5.10",
        "@storybook/addon-postcss": "2.0.0",
        "@storybook/react": "6.5.10",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "12.1.5",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@vitejs/plugin-react": "^2.0.0",
        "autoprefixer": "10.4.0",
        "babel-loader": "8.2.3",
        "chromatic": "6.7.4",
        "git-cz": "4.9.0",
        "html-webpack-plugin": "5.5.0",
        "husky": "8.0.1",
        "jsdom": "20.0.0",
        "path": "0.12.7",
        "postcss": "8.4.14",
        "prettier": "2.4.1",
        "react": "17.0.0",
        "react-dom": "17.0.0",
        "rimraf": "3.0.2",
        "rollup": "2.40.0",
        "rollup-plugin-dts": "4.2.2",
        "rollup-plugin-peer-deps-external": "2.2.4",
        "rollup-plugin-postcss": "4.0.2",
        "rollup-plugin-svg": "2.0.0",
        "rollup-plugin-terser": "7.0.2",
        "rollup-plugin-typescript2": "0.32.1",
        "rollup-plugin-visualizer": "4.2.0",
        "tailwindcss": "2.2.19",
        "typescript": "4.3.2",
        "vite": "^3.0.4",
        "vitest": "^0.20.3",
        "downlevel-dts": "^0.10.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/git-cz"
        }
    }
}
